alias cls='clear'
alias ex='exit'
alias finder='open /System/Library/CoreServices/Finder.app'
alias git-branch='git branch'
alias git-fetch='git fetch --all --tags'
alias git-lsremote='git for-each-ref refs/remotes --format="%(HEAD) %(align:80,left) %(color:bold yellow) %(refname:short) %(end) %(color:green) %(committerdate) %(color:dim white)%09-%(authorname)" \
	                                                --sort authorname committerdate' 
alias git-prune-remote='git remote prune origin'
alias ll='ls -lh'
alias la='ls -alh'
alias mvn-c='mvn clean'
alias mvn-cc='mvn clean compile'
alias mvn-ci='mvn clean install'
alias mvn-cp='mvn clean package'
alias pip='pip3.12'
alias py='python3.12'
alias sed='gsed'
alias sshfs='ssh wpnx777-fs1'
alias tf='terraform'
alias vi='vim'
